# Environment Configuration Example
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# App Environment (development | staging | production)
APP_ENV=development

# Version/Release name (e.g., branch name or git tag)
REF_NAME=local-dev

# Translations CDN
TRANSLATIONS_CDN_URL=
R2_PROJECT_NAME=
CROWDIN_BRANCH_NAME=

# Remote Config URL (Firebase or custom endpoint)
REMOTE_CONFIG_URL=
REMOTE_CONFIG_URL_STAGING=
REMOTE_CONFIG_URL_PROD=

# Google Drive Integration (optional)
GD_CLIENT_ID=
GD_APP_ID=
GD_API_KEY=

# Datadog RUM & Session Replay
DATADOG_APPLICATION_ID=
DATADOG_CLIENT_TOKEN=
DATADOG_SESSION_SAMPLE_RATE=10
DATADOG_SESSION_REPLAY_SAMPLE_RATE=1

# Analytics
RUDDERSTACK_KEY=
GROWTHBOOK_CLIENT_KEY=
GROWTHBOOK_DECRYPTION_KEY=

# Error Tracking
TRACKJS_TOKEN=
